#-------------------------------------------------------------------------------
#


PROGS = epm-calculator 

all: $(PROGS)

CC = LANG=C gcc -g  

#-------------------------------------------------------------------------------

epm-calculator: epm-calculator.c T/epm_bin_access.o
	$(CC) -o $@ $^  -lm


T/epm_bin_access.o: epm_bin_access.c epm_bin_access.h
	$(CC) -c -o $@ $<

#-------------------------------------------------------------------------------

clean:
	rm -f *.o T/*.o $(PROGS) 

#-------------------------------------------------------------------------------
#-------------------------------------------------------------------------------
