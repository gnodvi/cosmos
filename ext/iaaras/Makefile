
#-------------------------------------------------------------------------------
#-------------------------------------------------------------------------------


ABMD_DIR   = ./abmd/R
EPHOLD_DIR = ./eph-old-access/R

#-------------------------------------------------------------------------------

all:  ABMD__ EPHOLD__ 

#-------------------------------------------------------------------------------


ABMD__  :
	(cd $(ABMD_DIR);   make)

EPHOLD__ :
	(cd $(EPHOLD_DIR); make)

#-------------------------------------------------------------------------------

clean : 
	(cd $(ABMD_DIR);   make clean)
	(cd $(EPHOLD_DIR); make clean)

#-------------------------------------------------------------------------------
#-------------------------------------------------------------------------------
